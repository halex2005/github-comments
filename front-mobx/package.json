{
  "name": "mobx-github-comments",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autobind-decorator": "^2.4.0",
    "axios": "^0.18.1",
    "classnames": "^2.2.6",
    "mobx": "^4.14.1",
    "mobx-react": "^5.4.4",
    "react": "^16.11.0",
    "react-dom": "^16.11.0"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.9",
    "@types/jest": "^24.0.20",
